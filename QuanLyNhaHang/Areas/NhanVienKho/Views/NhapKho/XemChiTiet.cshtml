@model QuanLyNhaHang.Models.PhieuNhap

<h2>Chi tiết phiếu nhập #@Model.MaPhieuNhap</h2>

<table>
    <thead>
        <tr>
            <th>Nguyên liệu</th>
            <th>Số lượng</th>
            <th>Giá nhập</th>
            <th>Thành tiền</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.ChiTietPhieuNhaps)
        {
            <tr>
                <td>@(item.NguyenLieu?.TenNguyenLieu)</td>
                <td>@item.SoLuongNhap</td>
                <td>@item.GiaNhap</td>
                <td>@item.ThanhTien</td>
            </tr>
        }
    </tbody>
</table>
